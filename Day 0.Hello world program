#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;
int main() {
   string inputString; // declare a variable to hold our input
   getline(cin, inputString); // get a line of input from cin and save it to our variable
  
   cin>>inputString;
   cout << "Hello, World. "<< endl;
   

   // Write the second line of output
   cout<<inputString;
   return 0;
}
